#!/bin/zsh
[[ $- != *i* ]] && return

stty -ixon

source $HOME/dotfiles/sh/environment
source $HOME/dotfiles/sh/aliases
source $HOME/dotfiles/zsh/funcs
source $HOME/dotfiles/zsh/config
source $HOME/dotfiles/zsh/completion

if [[ -s /usr/bin/virtualenvwrapper_lazy.sh ]]; then
    source /usr/bin/virtualenvwrapper_lazy.sh
fi
